<div class="green container">
    <h1 class="text-center">Your plans</h1>
    <%if @plans.count !=0%>
        <%@plans.each do |p|%>
            <ul>
                <li>
                    <h4><%=link_to p.name, plan_path(p.id)%></h4>
                    <small>created <%=p.created_at.strftime("%B %d, %Y")%></small>
                </li>
            </ul>
        <%end%>
    <%else%>
        <p class="text-center">You don't have any plans yet...</p>
    <%end%>
</div>